{
  "name": "typecraft",
  "version": "1.0.0",
  "description": "Minecraft Clone in TypeScript",
  "main": "src/index.ts",
  "scripts": {
    "test": "ts-node test.ts",
    "build": "tsc",
    "start": "ts-node src/index.ts",
